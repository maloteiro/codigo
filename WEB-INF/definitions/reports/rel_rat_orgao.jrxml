	<queryString><![CDATA[SELECT 	LPAD(seq_rat,6,'0') seq_rat,
        dsc_responsavel_nome,
	dsc_gerente_nome,
	dat_data_cadastro,
	nom_local_nome,
	dsc_tipo_atividade,
	flg_rat_situacao,
	flg_rat_avaliacao_execucao,
        seq_local
FROM vw_rat
WHERE MONTH(dat_data_cadastro) = '$P!{tmp_mes}'
AND YEAR(dat_data_cadastro) = '$P!{tmp_ano}'
AND (seq_local = '$P!{tmp_seq_local}' or ''='$P!{tmp_seq_local}')
ORDER BY seq_local,seq_rat]]></queryString>
